public class Main
{
     public static int GCD(int n1, int n2) {
         int c=0;
         for(int i=n1;i>=1;i--){
             if(n1%i==0 && n2%i==0) {
                 c=i;
                 break;
             }
         }
         return c;
         

    }
	public static void main(String[] args) {
		System.out.print(GCD(19,38));
	}
}
