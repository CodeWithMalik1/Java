
import java.util.*;
public class Main
{
    public static void HollowRectanle(int row,int column){
        for(int i=0;i<row;i++){
            for(int j=0;j<column;j++){
                if(j==0||j==column-1||i==0||i==row-1)
                System.out.print("*");
                else System.out.print(" ");
            }
            System.out.println();
        }
    }
	public static void main(String[] args) {
	  int row=5,column=5;
	HollowRectanle(row,column);
	}
}
