
import java.util.*;
public class Main
{
    public static void InvertedHalfRotatedpyramid(int row,int column){
        int target=column-1;
        for(int i=0;i<row;i++){
            for(int j=0;j<column;j++){
                if(j+i>=target)
                System.out.print("*");
                else System.out.print(" ");
            }
            System.out.println();
        }
    }
	public static void main(String[] args) {
	  int row=5,column=5;
	  InvertedHalfRotatedpyramid(row,column);
	}
}
