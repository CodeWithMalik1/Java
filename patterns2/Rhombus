public class Main
{
    public static void solidRhombus(int n){
        for (int i=1;i<=n;i++){
         
            for(int spaces=1;spaces<=n-i;spaces++){
                System.out.print(" ");
            }
            for(int j=1;j<=n;j++){
                
                System.out.print("*");
                
            }
            System.out.println();
        } 
        
    }
	public static void main(String[] args) {
	    int n=5;
	    solidRhombus(n);
	}
}
