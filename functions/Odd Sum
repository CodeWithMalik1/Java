import java.util.*;
public class Main
{
    public static double oddSum(int n,Scanner sc){
        int sum=0,c=0;
        for(int i=0;i<n;i++){
            int num=sc.nextInt();
            if(num%2==0){
                sum+=num;
                c++;
            }
        }
        return sum/c;
        
    }
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
	System.out.print(oddSum(n,sc));
	}
}
