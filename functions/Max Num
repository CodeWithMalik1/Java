import java.util.*;
public class Main
{
    public static int Maxnum(int n,Scanner sc){
        int max=Integer.MIN_VALUE;
        for(int i=0;i<n;i++){
            int num=sc.nextInt();
            if(num>max){
                max=num;
            }
        }
        return max;
        
    }
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
	System.out.print(Maxnum(n,sc));
	}
}
